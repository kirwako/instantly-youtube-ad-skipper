{
	"manifest_version": 3,
	"name": "instantly Youtube Ad Skipper",
	"version": "1.0",
	"description": "Skips all Youtube ads | removes banner and sidebar ads | skips ads instantly",
	"content_scripts": [
		{
			"js": ["scripts/skip.js"],
			"matches": ["https://www.youtube.com/*", "https://m.youtube.com/*"]
		}
	],
	"icons": {
		"16": "images/icon16.png",
		"32": "images/icon32.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	}
}
